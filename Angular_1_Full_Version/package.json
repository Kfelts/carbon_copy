{
  "name": "angularJS-carboncopy",
  "version": "1.0.0",
  "description": "QuickStart package.json",
  "scripts": {
    "build": "tsc -p src/",
    "build:watch": "tsc -p src/ -w",
    "build:e2e": "tsc -p e2e/",
    "serve": "lite-server -c=bs-config.json",
    "serve:e2e": "lite-server -c=bs-config.e2e.json",
    "prestart": "npm run build",
    "start": "concurrently \"npm run build:watch\" \"npm run serve\"",
    "pree2e": "npm run build:e2e",
    "e2e": "concurrently \"npm run serve:e2e\" \"npm run protractor\" --kill-others --success first",
    "preprotractor": "webdriver-manager update",
    "protractor": "protractor protractor.config.js",
    "pretest": "npm run build",
    "test": "concurrently \"npm run build:watch\" \"karma start karma.conf.js\"",
    "pretest:once": "npm run build",
    "test:once": "karma start karma.conf.js --single-run",
    "lint": "tslint ./src/**/*.ts -t verbose"
  },
  "keywords": [],
  "author": "Alexander Sudarikov",
  "license": "MIT",
  "dependencies": {
    "@angular/common": "^16.2.5",
    "@angular/compiler": "^16.2.5",
    "@angular/core": "^16.2.5",
    "@angular/forms": "^16.2.5",
    "@angular/platform-browser": "^16.2.5",
    "@angular/platform-browser-dynamic": "^16.2.5",
    "@angular/router": "^16.2.5",
    "angular-in-memory-web-api": "^0.16.0",
    "core-js": "^3.32.2",
    "rxjs": "^7.8.1",
    "systemjs": "^6.14.2",
    "systemjs-plugin-babel": "^0.0.25",
    "zone.js": "^0.13.3"
  },
  "devDependencies": {
    "@types/jasmine": "^4.3.5",
    "@types/node": "^20.6.0",
    "canonical-path": "^1.0.0",
    "concurrently": "^8.2.1",
    "eslint": "^8.49.0",
    "jasmine-core": "^5.1.1",
    "karma": "^6.4.2",
    "karma-chrome-launcher": "^3.2.0",
    "karma-cli": "^2.0.0",
    "karma-jasmine": "^5.1.0",
    "karma-jasmine-html-reporter": "^2.1.0",
    "lite-server": "^2.6.1",
    "lodash": "^4.17.21",
    "rimraf": "^5.0.1",
    "selenium": "^2.20.0",
    "typescript": "^5.2.2"
  },
  "repository": {}
}
